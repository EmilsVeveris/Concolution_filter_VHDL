# include <stdio.h>
# include "altera_avalon_pio_regs.h"
# include "system.h"
# include "altera_avalon_uart_regs.h"


void delay(int a)
{
	volatile int i = 0;
	while (i < a*1000000)
	{
		i++;

	}
}

